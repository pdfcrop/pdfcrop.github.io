{
  "name": "pdfcrop-web",
  "version": "0.1.0",
  "description": "WebAssembly-powered PDF cropping application",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "wasm": "wasm-pack build --target web --release",
    "clean": "rm -rf dist pkg node_modules",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.6.0",
    "vite": "^6.0.0",
    "vite-plugin-top-level-await": "^1.4.4",
    "vite-plugin-wasm": "^3.3.0"
  },
  "dependencies": {
    "pdfjs-dist": "^4.0.379"
  },
  "keywords": [
    "pdf",
    "crop",
    "wasm",
    "webassembly",
    "rust",
    "typescript"
  ],
  "author": "pdfcrop contributors",
  "license": "MIT OR Apache-2.0"
}
